    "@react-navigation/drawer": "^7.5.8",
    "@react-navigation/native": "^7.0.14",
    "@reduxjs/toolkit": "^2.7.0",
    "@shopify/flash-list": "2.0.2",
    "axios": "^1.9.0",
    "buffer": "^6.0.3",
    "expo": "^54.0.7",
    "expo-av": "~16.0.7",
    "expo-blur": "~15.0.7",
